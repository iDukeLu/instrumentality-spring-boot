            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <!-- central -->
        <repository>
            <id>repo1</id>
            <url>https://repo1.maven.org/maven2</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>central</id>
            <url>http://central.maven.org/maven2</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <!-- private -->
        <repository>
            <id>rdc-releases</id>
            <url>https://repo.rdc.aliyun.com/repository/118184-release-WMJsKn/</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
